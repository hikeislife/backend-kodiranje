        </summary>
        
      </details>
      <details>
        <summary>
          <p>C#</p>
        </summary>
        {{#each publishedArticles}}
          {{#if (eq courseName "csharp")}}
            <p><a href="{{courseName}}/{{selectedURL}}">{{navName}}</a></p>
          {{/if}}
        {{/each}}
