<article>
  <h1>Lista svih artikala</h1>
  <a href="/admin/dodaj-lekciju" class="nocolor"><button class="wideAdminButton admin-newButton">Dodaj novu lekciju</button></a>
  <div class="column">
    <div class="published">
      <h2>Objavljeni</h2>
      <details>
        <summary>
          <p>Front page</p>
        </summary>
        {{#each publishedArticles}}
          {{#if (eq courseName "mp")}}
            <p><a href="{{courseName}}/{{selectedURL}}">{{navName}}</a></p>
          {{/if}}
        {{/each}}
      </details>
      <details>
        <summary>
          <p>Html i css</p>
        </summary>
        {{#each publishedArticles}}
          {{#if (eq courseName "html-i-css")}}
            <p><a href="{{courseName}}/{{selectedURL}}">{{navName}}</a></p>
          {{/if}}
        {{/each}}
      </details>
      <details>
        <summary>
          <p>JavaScript</p>
        </summary>
        {{#each publishedArticles}}
          {{#if (eq courseName "javascript")}}
            <p><a href="{{courseName}}/{{selectedURL}}">{{navName}}</a></p>
          {{/if}}
